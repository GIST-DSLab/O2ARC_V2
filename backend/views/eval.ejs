<body>
      <!--Main Navigation-->
    <header>

        <nav class="navbar navbar-expand-lg navbar-light bg-white fixed-top ">
        <div class="container-fluid">
            
    <div id="main">
        <div id="menu">
        <div class="boxed">
            <h3>TASK NAME</h3>
            <label id='current_task_name' for="random_task_btn"></label>
            <label id='current_task_desc' for="random_task_btn"> </label>
            </div>
            <br/>
            <br />
            <br/>
            <div class="logpair">
                <div class="loggrid" id="input_grid"></div>
                    <h4>&#8594;</h4>
                <div class="loggrid" id="output_grid"></div>
            </div>
            <div class = "overview" id= "overview"><a id = "active" >OVERVIEW</a></div>

        </p>
        </div>
        <div id="visualization">
            <div class = "overview_page" id ='overview_page' hidden = "false">
                <div class = "training_text">TRAINING EXAMPLES</div>

                <div class="test_grids" id = "testview"></div>
            </div>
            <div class="grid" id="show_grid">
